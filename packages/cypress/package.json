{
  "name": "@replayio/cypress",
  "version": "3.0.5",
  "description": "Plugin to record your Cypress tests with Replay",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./support": "./dist/support.js",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "pkg-build",
    "test": "echo \"Error: no test specified\"",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/replayio/replay-cli.git"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/replayio/replay-cli/issues"
  },
  "homepage": "https://github.com/replayio/replay-cli/blob/main/packages/cypress/README.md",
  "devDependencies": {
    "@replay-cli/shared": "workspace:^",
    "@replay-cli/tsconfig": "workspace:^",
    "@types/debug": "^4.1.7",
    "@types/node": "^20.11.27",
    "@types/semver": "^7.3.13",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.10",
    "cypress": "^13.11.0",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@replayio/test-utils": "workspace:^",
    "chalk": "^4.1.2",
    "debug": "^4.3.4",
    "fs-extra": "^11.2.0",
    "semver": "^7.5.2",
    "terminate": "^2.6.1",
    "txml": "^3.2.5",
    "undici": "^5.28.4",
    "uuid": "^8.3.2",
    "winston": "^3.13.0",
    "winston-loki": "^6.1.2",
    "ws": "^8.14.2"
  },
  "peerDependencies": {
    "cypress": "^13"
  },
  "@replay-cli/pkg-build": {
    "entrypoints": [
      "./src/index.ts",
      "./src/support.ts"
    ]
  }
}
