{
  "name": "@replayio/cypress",
  "version": "2.1.3",
  "description": "Plugin to record your Cypress tests with Replay",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./support": "./dist/support.js",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "rm -rf dist/ tsconfig.tsbuildinfo && tsc",
    "test": "echo \"Error: no test specified\"",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/replayio/replay-cli.git"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/replayio/replay-cli/issues"
  },
  "homepage": "https://github.com/replayio/replay-cli/blob/main/packages/cypress/README.md",
  "devDependencies": {
    "@replay-cli/tsconfig": "workspace:^",
    "@types/debug": "^4.1.7",
    "@types/node": "^20.11.27",
    "@types/semver": "^7.3.13",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.10",
    "cypress": "^10.9.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@replayio/replay": "workspace:^",
    "@replayio/test-utils": "workspace:^",
    "chalk": "^4.1.2",
    "debug": "^4.3.4",
    "semver": "^7.5.2",
    "terminate": "^2.6.1",
    "txml": "^3.2.5",
    "uuid": "^8.3.2",
    "ws": "^8.14.2"
  },
  "peerDependencies": {
    "cypress": ">=5.3.0"
  }
}
